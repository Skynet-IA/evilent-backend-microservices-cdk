    "aws-cdk-lib": "^2.221.0",
    "aws-jwt-verify": "^5.1.1",
    "aws-lambda": "^1.0.7",
    "axios": "^1.13.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.156",
