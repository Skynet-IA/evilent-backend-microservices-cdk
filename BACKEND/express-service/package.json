{
  "name": "express-service",
  "version": "1.0.0",
  "description": "Express.js Monolith Template - Base replicable para proyectos sencillos",
  "main": "dist/bin/express-service.js",
  "scripts": {
    "dev": "ts-node src/bin/express-service.ts",
    "build": "tsc",
    "start": "node dist/bin/express-service.js",
    "test": "jest --coverage",
    "test:unit": "jest test/unit --coverage",
    "test:integration": "jest test/integration",
    "test:e2e": "jest test/e2e",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'"
  },
  "keywords": [
    "express",
    "nodejs",
    "typescript",
    "cognito",
    "template"
  ],
  "author": "EVILENT Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pg": "^8.16.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
